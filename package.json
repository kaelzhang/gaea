{
  "name": "gaea",
  "version": "5.0.4",
  "description": "The manager to wrap grpc services and hold .proto files.",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_DEBUG=gaea nyc ava --timeout=10s --verbose",
    "test:dev": "NODE_DEBUG=gaea nyc ava --timeout=10s --verbose && npm run report:dev",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "posttest": "npm run report",
    "report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "report:dev": "nyc report --reporter=html && npm run report:open",
    "report:open": "open coverage/index.html"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/gaea.git"
  },
  "keywords": [
    "gaea",
    "grpc",
    "server",
    "protobuf",
    "manager",
    "protobuf files holder"
  ],
  "engines": {
    "node": ">=10"
  },
  "ava": {
    "babel": false,
    "files": [
      "test/*.test.js"
    ]
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/gaea/issues"
  },
  "devDependencies": {
    "@ostai/eslint-config": "^3.3.2",
    "ava": "^2.2.0",
    "codecov": "^3.5.0",
    "egg-mysql": "^3.0.0",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.1",
    "core-util-is": "^1.0.2",
    "err-object": "^5.1.2",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "grpc": "^1.22.2",
    "make-array": "^1.0.5",
    "object-access": "^2.0.4",
    "resolve-from": "^5.0.0",
    "skema": "^9.4.2"
  }
}
